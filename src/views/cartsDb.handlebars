<nav class="navbar bg-dark bg-body-tertiary">
  <div class="container-fluid">
    <button
      class="btn border border-body text-light"
      id="login"
      name="login"
    >Iniciar sesion</button>
    <h5 class="text-light me-5" id="logged"></h5>
  </div>
</nav>
<div class="container">
  <h3 class="col-3 mt-2">{{cart.email}}</h3>
  <div class="row">
    
    {{#each cart.products}}
      <div class="card my-2 mx-1" style="width: 18rem;">
        <div class="card-body">
          <h4 class="card-title">{{this.product.title}}</h4>
          <h6 class="card-subtitle my-3 text-muted">{{this.product.description}}</h6>
          <p class="card-text">Precio: {{this.product.price}}</p>
          <p class="card-text">Precio: {{this.product.code}}</p>
          <p class="card-text">Precio: {{this.product.stock}}</p>
          <p class="card-text">Precio: {{this.product.category}}</p>
          <h class="card-subtitle text-muted">Cantidad:{{this.quantity}}</h>
        </div>
      </div>
    {{/each}}
  </div>
</div>

